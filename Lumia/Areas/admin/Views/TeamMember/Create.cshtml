
@{
    ViewData["Title"] = "Create";
}
@model TeamMember
<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Name">Fullname</label>
        <input class="form-control" asp-for="Name" placeholder="Enter name">
    </div>
    <div class="form-group">
        <label asp-for="Position">Position</label>
        <input class="form-control" asp-for="Position" placeholder="Enter position">
    </div>
    <div class="form-group">
        <label asp-for="Title">Title</label>
        <input class="form-control" asp-for="Title" placeholder="Enter title">
    </div>
    <div class="form-group">
        <label asp-for="FormFIle">Image</label>
        <input class="form-control" asp-for="FormFIle" accept="image/*">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

